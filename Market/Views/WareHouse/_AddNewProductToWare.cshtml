@model ProductWareShortModel




 <div class="modal-content">
      <div class="modal-header">
        <h5 class="modal-title" id="exampleModalLabel">Add new product to ware</h5>
        <button type="button" class="btn-close" data-bs-dismiss="modal" aria-label="Close"></button>
      </div>
      <div class="modal-body">
        <div class="mb-3">
          <label for="dropdown" class="form-label">Product name</label>
          <select class="form-select" aria-label="Dropdown" asp-for="ProductId" asp-items="(SelectList)ViewBag.Products">
          </select>
        </div>
        <div class="mb-3">
          <label for="input" class="form-label">Input</label>
          <input type="text" class="form-control" asp-for="Count" placeholder="Input">
        </div>
      </div>
      <div class="modal-footer">
        <button type="button" class="btn btn-secondary" data-bs-dismiss="modal">Cancel</button>
        <button type="button" id="addProductToWare" class="btn btn-primary">Save</button>
      </div>
    </div>